{
  "expo": {
    "name": "Hyperliquid Mobile",
    "slug": "hyperliquid-mobile-companion",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "hyperliquidmobile",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hyperliquid.mobile",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "metamask",
          "trust",
          "rainbow",
          "coinbase",
          "cbwallet",
          "zerion",
          "ledgerlive",
          "blockchain",
          "exodus",
          "argent",
          "gnosis",
          "safe",
          "binance",
          "bitget",
          "okx",
          "bybit",
          "imtoken",
          "tokenpocket",
          "alphawallet",
          "unstoppable",
          "zengo",
          "phantom",
          "brave",
          "bitpay",
          "kraken",
          "cryptocom",
          "mathwallet",
          "1inch",
          "pillar",
          "onto",
          "tokenary",
          "ownbit",
          "gnosis",
          "mew",
          "slope",
          "uniswap",
          "crypto",
          "safepal",
          "keplr",
          "coin98",
          "dcentwallet",
          "bitkeep",
          "coinus",
          "kleverwallet",
          "steak",
          "celo",
          "valora",
          "frontier",
          "opera-crypto",
          "status"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hyperliquid.mobile",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "34c623ee-b870-403d-9c2a-6cd20eb4a625"
      }
    },
    "owner": "cezarrr"
  }
}
